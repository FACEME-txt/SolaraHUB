local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
	Name = "Solara HUB",
	LoadingTitle = "Solara HUB Suite",
	LoadingSubtitle = "by owner",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = "Rayfield Interface Suite",
		FileName = "Big Hub"
	},
	KeySystem = true, -- Set this to true to use their key system
	KeySettings = {
		Title = "Solara HUB",
		Subtitle = "Key System",
		Note = "Join the discord (https://discord.gg/PuYkPXsSFY)",
		SaveKey = true,
		Key = "ISAKDASJDASWIY2412I3SDJAKDKGDSJFALKWEISDJ"
	}
})

Rayfield:Notify("Title Example", "Content/Description Example", 4483362458) -- Notfication -- Title, Content, Image

